
<div class="form_div">
    <%= form_with model: @airplane, local: true do |f| %>
        <%= f.label :name %>
        
        <%= f.text_field :name %>
        
        <%= f.label :total_rows %>

        <%= f.text_field :total_rows %>
        
        <%= f.label :total_columns %>
        
        <%= f.text_field :total_columns %>
        

        <%= f.submit %>
    <% end %>
</div>