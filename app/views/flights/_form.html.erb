<div class="form_div">
    <%= form_with model: @library, local: true, html: { multipart: true } do |f| %>
        <%= f.label :origin %>
        
        <%= f.text_field :origin %>
        
        <%= f.label :destination %>
       
        <%= f.text_field :destination %>
        
        <%= f.label :date %>
        
        <%= f.date_field :date %>
        
        <%= f.label :airplane_id %>
       
        <%= f.text_field :airplane_id %>
      

        
        <%= f.submit %>
    <% end %>
</div>